<div class="container">

<table  class="table"  >
  <thead>
    <tr>
       <th scope="col" >#</th>
     <th scope="col" >First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col" >Favorite Color </th>
    </tr>
  </thead>
  <tbody>
  
<% people.each do |p|%>
    <tr>
        <td><%= p.id%></td>
        <td><%= p.first_name%></td>
        <td><%= p.last_name%></td>
        <td><%= p.fav_color%></td>
    </tr>
<%end%>
  </tbody>
</table>

</div>